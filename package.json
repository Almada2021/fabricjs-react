  "devDependencies": {
    "@types/fabric": "^4.5.12",
    "@types/node": "^18.11.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^2.2.0",
    "path": "^0.12.7",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1"
  },
  "bugs": {
    "url": "https://github.com/asotog/fabricjs-react/issues"
